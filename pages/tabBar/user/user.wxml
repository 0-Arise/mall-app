<view><block wx:if="{{showHeader}}"><view class="status" style="{{'position:'+(headerPosition)+';'+('top:'+(statusTop)+';')}}"></view></block><block wx:if="{{showHeader}}"><view class="header" style="{{'position:'+(headerPosition)+';'+('top:'+(headerTop)+';')}}"><view class="icon-btn"><view data-event-opts="{{[['tap',[['toMsg',['$event']]]]]}}" class="icon tongzhi" bindtap="__e"></view><view data-event-opts="{{[['tap',[['toSetting',['$event']]]]]}}" class="icon setting" bindtap="__e"></view></view></view></block><block wx:if="{{showHeader}}"><view class="place"></view></block><view class="user"><view class="left"><block wx:if="{{provider=='weixin'}}"><view><open-data class="user-avatar" type="userAvatarUrl"></open-data></view></block><block wx:if="{{provider=='toutiao'}}"><view><image src="{{user.face}}" data-event-opts="{{[['tap',[['toSetting',['$event']]]]]}}" bindtap="__e"></image></view></block><block wx:if="{{provider==null}}"><view><image src="{{user.face}}" data-event-opts="{{[['tap',[['toSetting',['$event']]]]]}}" bindtap="__e"></image></view></block></view><view class="right"><view data-event-opts="{{[['tap',[['toLogin',['$event']]]]]}}" class="username" bindtap="__e"><block wx:if="{{provider=='weixin'}}"><view><open-data type="userNickName"></open-data></view></block><block wx:if="{{provider=='toutiao'}}"><view><view data-event-opts="{{[['tap',[['toLogin',['$event']]]]]}}" class="username" bindtap="__e">{{user.name}}</view><view data-event-opts="{{[['tap',[['toSetting',['$event']]]]]}}" class="signature" bindtap="__e">{{user.signature}}</view></view></block><block wx:if="{{provider==null}}"><view><view data-event-opts="{{[['tap',[['toLogin',['$event']]]]]}}" class="username" bindtap="__e">{{user.name}}</view><view data-event-opts="{{[['tap',[['toSetting',['$event']]]]]}}" class="signature" bindtap="__e">{{user.signature}}</view></view></block></view></view><view data-event-opts="{{[['tap',[['toMyQR',['$event']]]]]}}" class="erweima" bindtap="__e"><view class="icon qr"></view></view></view><view class="VIP"><view class="img"><image src="/static/img/VIP.png"></image></view><view class="title">{{vipName+"会员"}}</view><view class="tis">会员特权</view></view><view class="order"><view class="list"><block wx:for="{{orderList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toOrderList',['$0'],[[['orderList','',index,'id']]]]]]]}}" class="box" bindtap="__e"><view class="img"><view class="{{['icon',row.icon]}}"></view></view><view class="text">{{row.text}}</view></view></block></view><block wx:if="{{1==1}}"><view class="balance-info"><view class="left"><view data-event-opts="{{[['tap',[['toIntegralList',[0,'积分']]]]]}}" class="box" bindtap="__e"><view class="num">{{user.integral}}</view><view class="text">积分</view></view><view data-event-opts="{{[['tap',[['toIntegralList',[1,'U币']]]]]}}" class="box" bindtap="__e"><view class="num">{{user.balance}}</view><view class="text">U币</view></view><view data-event-opts="{{[['tap',[['toIntegralList',[2,'佣金']]]]]}}" class="box" bindtap="__e"><view class="num">{{user.envelope}}</view><view class="text">佣金</view></view></view><view class="right"><view data-event-opts="{{[['tap',[['toSignin',['$event']]]]]}}" class="box" bindtap="__e"><view class="img"><view class="icon chongzhi"></view></view><view class="text">签到</view></view></view></view></block></view><block wx:if="{{1==1}}"><view class="toolbar"><view class="title">我的工具栏</view><view class="list"><block wx:for="{{mytoolbarList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toPage',['$0','$1'],[[['mytoolbarList','',index,'url']],[['mytoolbarList','',index,'id']]]]]]]}}" class="box" bindtap="__e"><view class="img"><image src="{{row.img}}"></image></view><view class="text">{{row.text}}</view></view></block></view></view></block><block wx:if="{{1==1}}"><view class="like-goods-list" style="{{'margin-top:'+(likeMarginTop)+';'}}"><view class="title"><image src="/static/img/hua.png"></image>为您推荐<image src="/static/img/hua.png"></image></view><view class="product-list"><block wx:for="{{likeList}}" wx:for-item="goods" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['likeList','id',goods.id]]]]]]]}}" class="product" bindtap="__e"><image mode="widthFix" src="{{goods.coverImgs}}"></image><view class="name">{{goods.name}}</view><view class="info"><view class="price">{{goods.price}}</view><view class="slogan">{{goods.slogan}}</view></view></view></block></view></view></block><view class="place-bottom"></view></view>